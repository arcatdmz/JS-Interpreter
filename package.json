{
  "name": "JS-Interpreter",
  "version": "0.1.2",
  "description": "A sandboxed JavaScript interpreter in JavaScript. Execute arbitrary JavaScript code line by line in isolation and safety.",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arcatdmz/JS-Interpreter.git"
  },
  "keywords": [
    "javascript",
    "js-interpreter",
    "sandbox"
  ],
  "author": "Jun Kato <i@junkato.jp> (https://junkato.jp)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/arcatdmz/JS-Interpreter/issues"
  },
  "homepage": "https://github.com/arcatdmz/JS-Interpreter#readme",
  "scripts": {
    "build": "webpack --config webpack.config --mode production",
    "prettier": "prettier --write {.,demos/**,src/**,tyrant/*}/*.{css,htm,html,js,json,md,scss,ts,yaml,yml}",
    "test": "node ./tyrant/pretest.js && js-interpreter-tyrant -drs --progress --hostPath ./tyrant/run.js",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@willbooster/prettier-config",
  "dependencies": {
    "@types/acorn": "4.0.5",
    "@types/estree": "0.0.39",
    "@types/node": "12.7.4",
    "acorn": "0.4.2"
  },
  "devDependencies": {
    "@code-dot-org/js-interpreter-tyrant": "0.2.2",
    "@willbooster/prettier-config": "0.1.1",
    "prettier": "1.18.2",
    "ts-loader": "6.0.4",
    "ts-node": "^8.3.0",
    "typescript": "3.6.2",
    "webpack": "4.39.3",
    "webpack-cli": "3.3.8"
  }
}
